<odoo>
    <template id="report_libro">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <h2>Libro</h2>
                Nombre: <t t-esc="doc.nombre"/>
                <br/>
                Descripción: <t t-esc="doc.descripcion"/>
                <br/>
                Fecha de salida: <t t-esc="doc.fecha_lanzamiento"/>
                <br/>
                Género: <t t-esc="doc.genero"/>
                <br/>
                Premio: <t t-esc="doc.id_premio"/>
            </t>
        </t>
    </template>
</odoo> 